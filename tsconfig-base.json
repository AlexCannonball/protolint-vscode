{
  "extends": "@tsconfig/node22/tsconfig.json",
  "compilerOptions": {
    "removeComments": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "composite": true,

    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}
